{
  "name": "zwaplace-api",
  "version": "0.0.1",
  "description": "",
  "main": "dist/index.js",
  "scripts": {
    "prestart": "cross-env NODE_ENV=development",
    "automigrate": "sequelize db:migrate --env development",
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch",
    "build": "npm run build:types && npm run build:js",
    "build:types": "tsc --emitDeclarationOnly",
    "build:js": "babel src --out-dir dist --extensions \".ts,.js\" --ignore src/extends/ --source-maps inline",
    "start": "cross-env NODE_ENV=development node dist/index.js",
    "dev": "nodemon src/index.ts",
    "test": "jest --coverage --config=jest.config.js  --runInBand --detectOpenHandles --forceExit",
    "test:CI": "CI=true npm test",
    "pretest": "npm run db:drop:test && npm run db:create:test && cross-env NODE_ENV=test npm run db:migrate",
    "coveralls": "cat coverage/lcov.info | coveralls",
    "sequelize": "./node_modules/.bin/babel-node ./node_modules/.bin/sequelize $*",
    "db:create": "./node_modules/.bin/sequelize db:create",
    "db:create:test": "cross-env NODE_ENV=test ./node_modules/.bin/sequelize db:create",
    "db:drop": "./node_modules/.bin/sequelize db:drop",
    "db:drop:test": "cross-env NODE_ENV=test ./node_modules/.bin/sequelize db:drop",
    "db:migrate": "./node_modules/.bin/sequelize db:migrate",
    "db:migrate:reset": "./node_modules/.bin/sequelize db:migrate:undo:all",
    "db:migrate:test": "cross-env NODE_ENV=test npm run db:migrate",
    "lint": "eslint src/",
    "lint:fix": "eslint --fix src/"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@gitlab.com/justicebot/zwaplace-api.git"
  },
  "author": "Justice ChatBot Ltd",
  "license": "ISC",
  "bugs": {
    "url": "https://gitlab.com/justicebot/zwaplace-api/issues"
  },
  "homepage": "https://gitlab.com/justicebot/zwaplace-api#readme",
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.4",
    "@babel/node": "^7.8.3",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.8.3",
    "@babel/polyfill": "^7.8.3",
    "@babel/preset-env": "^7.8.4",
    "@babel/preset-typescript": "^7.8.3",
    "@babel/register": "^7.8.3",
    "@types/body-parser": "^1.17.1",
    "@types/cors": "^2.8.6",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.2",
    "@types/morgan": "^1.7.37",
    "@types/node": "^13.5.0",
    "acorn": "^6.0.0",
    "acorn-dynamic-import": "^4.0.0",
    "babel-watch": "^7.0.0",
    "cross-env": "^6.0.3",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-prettier": "^6.9.0",
    "eslint-plugin-import": "^2.20.0",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-typescript": "^0.14.0",
    "husky": "^4.0.10",
    "jest": "^24.9.0",
    "lint-staged": "^9.5.0",
    "prettier": "^1.19.1",
    "supertest": "^4.0.2",
    "ts-loader": "^6.2.1",
    "ts-node": "^8.6.2",
    "typescript": "^3.7.5",
    "typescript-eslint-parser": "^22.0.0",
    "webpack": "^4.41.5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && npm run lint",
      "pre-push": "git diff HEAD --quiet & npm test"
    }
  },
  "lint-staged": {
    "src/**/*.{js,ts,json}": [
      "prettier --write",
      "git add"
    ]
  },
  "dependencies": {
    "@hapi/joi": "^17.1.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/swagger-ui-express": "^4.1.1",
    "axios": "^0.19.2",
    "babel-loader": "^8.0.6",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "http-errors": "^1.7.3",
    "install": "^0.13.0",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.29.1",
    "morgan": "^1.9.1",
    "mysql2": "^2.1.0",
    "nodemon": "^2.0.2",
    "npm": "^6.13.7",
    "path": "^0.12.7",
    "sequelize": "^5.21.3",
    "sequelize-cli": "^5.5.1",
    "snake-case": "^3.0.4",
    "socket.io": "^2.3.0",
    "swagger-ui-express": "^4.1.3"
  }
}
